            motion:layout_constraintStart_toStartOf="@+id/left_whit_eye"
            motion:layout_constraintTop_toTopOf="@+id/left_whit_eye" />

        <Constraint
            android:id="@+id/vTopRightEye"
            android:layout_width="1dp"
            android:layout_height="1dp"
            motion:layout_constraintBottom_toTopOf="@+id/right_whit_eye"
            motion:layout_constraintLeft_toLeftOf="@+id/right_whit_eye"
            motion:layout_constraintRight_toRightOf="@+id/right_whit_eye"
            motion:layout_constraintTop_toTopOf="@+id/right_whit_eye" />

        <Constraint
            android:id="@+id/vBottomRightEye"
            android:layout_width="1dp"
            android:layout_height="1dp"
            motion:layout_constraintBottom_toBottomOf="@+id/right_whit_eye"
