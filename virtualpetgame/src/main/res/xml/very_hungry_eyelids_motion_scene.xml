<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <Transition
        app:constraintSetEnd="@+id/end"
        app:constraintSetStart="@+id/start"
        app:duration="250" />

    <ConstraintSet android:id="@+id/start">

        <Constraint
            android:id="@+id/veryHungryRightTopEyelid"
            android:layout_width="40dp"
            android:layout_height="0dp"
            app:layout_constraintLeft_toRightOf="@+id/vVeryHungryEyelidsCenter"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/vVeryHungryEyelidsCenter"
            app:layout_constraintTop_toTopOf="@+id/vVeryHungryEyelidsCenter"/>


        <Constraint
            android:id="@+id/vVeryHungryEyelidsCenter"
            android:layout_width="10dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Constraint
            android:id="@+id/veryHungryLeftTopEyelid"
            android:layout_width="40dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/vVeryHungryEyelidsCenter"
            app:layout_constraintRight_toLeftOf="@+id/vVeryHungryEyelidsCenter"
            app:layout_constraintTop_toTopOf="parent" />




    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">

        <Constraint
            android:id="@+id/veryHungryRightTopEyelid"
            android:layout_width="40dp"
            android:layout_height="0dp"
            app:layout_constraintLeft_toRightOf="@+id/vVeryHungryEyelidsCenter"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>


        <Constraint
            android:id="@+id/vVeryHungryEyelidsCenter"
            android:layout_width="10dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"  />

        <Constraint
            android:id="@+id/veryHungryLeftTopEyelid"
            android:layout_width="40dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/vVeryHungryEyelidsCenter"
            app:layout_constraintTop_toTopOf="parent" />


    </ConstraintSet>
</MotionScene>
